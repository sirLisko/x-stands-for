{
  "manifest_version": 3,
  "name": "X stands for",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*", "https://x.com/*"],
      "css": ["style.css"],
      "js": ["content-script.js"],
      "run_at": "document_end"
